forwards auth to configured host:
  1: |
    apiVersion: traefik.io/v1alpha1
    kind: Middleware
    metadata:
      labels:
        app: platform-ingress
        chart: platform-ingress-1.0.0
        heritage: Helm
        release: ingress
        service: traefik
      name: ingress-ingress-auth
    spec:
      forwardAuth:
        address: https://api.example.com/oauth/authorize
        trustForwardHeader: true
