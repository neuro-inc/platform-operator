should match snapshot:
  1: |
    apiVersion: argoproj.io/v1alpha1
    kind: Application
    metadata:
      labels:
        app: platform
        chart: platform-1.0.0
        heritage: Helm
        release: platform
      name: platform-monitoring
    spec:
      destination:
        namespace: platform
        server: https://kubernetes.default.svc
      project: default
      source:
        chart: platform-monitoring
        helm:
          valuesObject:
            fullnameOverride: platform-monitoring
            image:
