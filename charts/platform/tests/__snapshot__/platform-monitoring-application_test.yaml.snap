              token:
                valueFrom:
                  secretKeyRef:
                    key: token
                    name: platform-token
            priorityClassName: platform-service
            securityContext:
              enabled: true
            sentry:
              clusterName: test-cluster
              dsn: https://sentry-dsn
              sampleRate: 0.1
