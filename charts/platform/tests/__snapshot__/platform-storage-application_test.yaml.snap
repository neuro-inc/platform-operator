should match snapshot:
  1: |
    apiVersion: argoproj.io/v1alpha1
    kind: Application
    metadata:
      labels:
        app: platform
        chart: platform-1.0.0
        heritage: Helm
        release: platform
      name: test-cluster--platform-storage
    spec:
      destination:
        server: https://kubernetes.default.svc
      project: default
      source:
        chart: platform-storage
        helm:
          releaseName: platform-storage
          valuesObject:
            image:
              repository: ghcr.io/neuro-inc/storageapi
            ingress:
              enabled: true
              hosts:
                - test-cluster.org.apolo.us
              ingressClassName: traefik
            platform:
              adminUrl: https://api.apolo.us
              authUrl: https://api.apolo.us
              clusterName: test-cluster
