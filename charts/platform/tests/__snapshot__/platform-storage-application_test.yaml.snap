                valueFrom:
                  secretKeyRef:
                    key: token
                    name: platform-token
            priorityClassName: platform-services
            securityContext:
              enabled: true
            sentry:
              clusterName: test-cluster
              dsn: https://sentry-dsn
              sampleRate: 0.1
            service:
              annotations:
                traefik.ingress.kubernetes.io/service.sticky.cookie: "true"
                traefik.ingress.kubernetes.io/service.sticky.cookie.name: NEURO_STORAGEAPI_SESSION
            storageUsageCollector:
              resources:
                limits:
                  cpu: 1000m
                  memory: 1Gi
                requests:
                  cpu: 250m
                  memory: 500Mi
            storages:
              - claimName: platform-storage-storage
                path: /storage
                type: pvc
        repoURL: ghcr.io/neuro-inc/helm-charts
        targetRevision: '*'
      syncPolicy:
        automated:
          enabled: true
          prune: false
          selfHeal: false
        managedNamespaceMetadata:
          annotations:
            argocd.argoproj.io/sync-options: Delete=false
        syncOptions:
          - CreateNamespace=true
          - PrunePropagationPolicy=foreground
