            image:
              name: ghcr.io/neuro-inc/traefik
            ingressClass:
              enabled: true
            ingressRoute:
              dashboard:
                enabled: false
            instanceLabelOverride: platform
            logs:
              general:
                level: ERROR
            metrics:
              prometheus:
                service:
                  enabled: true
                serviceMonitor:
                  additionalLabels:
                    platform.apolo.us/scrape-metrics: "true"
